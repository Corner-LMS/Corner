{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
 
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_NAME": "Corner Dev"
      }
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_NAME": "Corner Preview"
      }
    },
    "production": {
      "autoIncrement": true
    },
    
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "environment": "development"
    }
  },
  "submit": {
    "production": {}
  }
}
